@model LivrariaOnline.ViewModels.IndexViewModel
@{
	ViewData["Title"] = "Home Page";
}

<div class="text-center">
	<h2>Welcome to Livraria Online</h2>
</div>

<header>
	<nav class="navbar bg-body-tertiary">
		<div class="container-fluid">
			<a class="navbar-brand btn" asp-area="" asp-controller="Books" asp-action="BookDetails" asp-route-id="0">Create a new book</a>
			<form class="d-flex" role="search">
				<input class="form-control me-2" type="search" value="@Model.Search" placeholder="Search" aria-label="Search" name="Search">
				<button class="btn btn-outline-success" value="Search" type="submit">Search</button>
			</form>
		</div>
	</nav>
</header>
<body>

	<div class="card-group">
		<div class="card">
			<img src="data:image/jpeg;base64,"
				 width="auto"
				 height="auto"
				 class="card-img-top">

			<div class="card-body">
				<h5 class="card-title">Harry Potter e a Pedra Filosofal</h5>
				<p class="card-text">J.K. ROWLING</p>
			</div>
			<div class="card-footer">
				<small class="text-body-secondary">16,90$</small>
			</div>
		</div>
		<div class="card">
			<img src="data:image/jpeg;base64,"
				 width="423"
				 height="300"
				 class="card-img-top">

			<div class="card-body">
				<h5 class="card-title">O Diário de um Banana 5: A Verdade Nua e Crua já chegou!</h5>
				<p class="card-text">Jeff Kinney</p>
			</div>
			<div class="card-footer">
				<small class="text-body-secondary">30,07$</small>
			</div>
		</div>
		<div class="card">
			<img src="data:image/jpeg;base64,"
				 width="340"
				 height="340"
				 class="card-img-top" alt="capa do livro neverwhere">

			<div class="card-body">
				<h5 class="card-title">Neverwhere</h5>
				<p class="card-text">Neil Gaiman</p>
			</div>
			<div class="card-footer">
				<small class="text-body-secondary">10,15$</small>
			</div>
		</div>
	</div>

</body>